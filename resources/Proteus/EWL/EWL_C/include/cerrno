#ifndef _EWL_CERRNO
#define _EWL_CERRNO

#include <ansi_parms.h>

#if !_EWL_USING_CW_C_HEADERS_
	#include <errno.h>
#else

#include <ewl_lib_ext1.h>

#if _AEABI_PORTABILITY_LEVEL
	#include <arm/cerrno_aeabi>
#endif

#if __dest_os == __starcore
#include <sc/errno_StarCore.h>
#endif

#ifndef EAGAIN
#define EAGAIN			11	/* Resource temporarily unavailable */
#endif

#ifndef EDOM
#define EDOM			33	/* Numerical argument out of domain */
#endif

#ifndef ERANGE
#define ERANGE			34	/* Result too large */
#endif

#ifndef EDEADLK
#define EDEADLK			35	/* Resource deadlock avoided */
#endif

#ifndef ENAMETOOLONG
#define ENAMETOOLONG	36	/* File name too long */
#endif

#ifndef ENOSYS
#define ENOSYS			38	/* Function not implemented */
#endif

#ifndef ENOTEMPTY
#define ENOTEMPTY		39	/* Directory not empty */
#endif

#if _EWL_C99
#ifndef EILSEQ
#define EILSEQ     		88	/* Wide character encoding error */
#endif
#endif /* _EWL_C99 */

#ifndef E2BIG
#define E2BIG			 7	/* Argument list too long */
#endif

#ifndef EACCES
#define EACCES			13	/* Permission denied */
#endif

#ifndef EBADF
#define EBADF			 9	/* Bad file descriptor */
#endif

#ifndef EBUSY
#define EBUSY			16	/* Device busy */
#endif

#ifndef ECHILD
#define ECHILD			10	/* No child processes */
#endif

#define EDEADLOCK	EDEADLK	/* Resource deadlock avoided */

#ifndef EEXIST
#define EEXIST			17	/* File exists */
#endif

#ifndef EFAULT
#define EFAULT			14	/* Bad address */
#endif

#ifndef EFBIG
#define EFBIG			27	/* File too large */
#endif

#ifndef EFPOS
#define EFPOS			40	/* File Position Error */
#endif

#ifndef EINTR
#define EINTR			 4	/* Interrupted system call */
#endif

#ifndef EINVAL
#define EINVAL			22	/* Invalid argument */
#endif

#ifndef EIO
#define EIO				 5	/* Input/output error */
#endif

#ifndef EISDIR
#define EISDIR			21	/* Is a directory */
#endif

#ifndef EMFILE
#define EMFILE			24	/* Too many open files */
#endif

#ifndef EMLINK
#define EMLINK			31	/* Too many links */
#endif

#ifndef ENFILE
#define ENFILE			23	/* Too many open files in system */
#endif

#ifndef ENODEV
#define ENODEV			19	/* Operation not supported by device */
#endif

#ifndef ENOENT
#define ENOENT			 2	/* No such file or directory */
#endif

#ifndef ENOERR
#define ENOERR			 0	/* No error detected */
#endif

#ifndef ENOEXEC
#define ENOEXEC			 8	/* Exec format error */
#endif

#ifndef ENOLCK
#define ENOLCK			77	/* No locks available */
#endif

#ifndef ENOMEM
#define ENOMEM			12	/* Cannot allocate memory */
#endif

#ifndef ENOSPC
#define ENOSPC			28	/* No space left on device */
#endif

#ifndef ENOTDIR
#define ENOTDIR			20	/* Not a directory */
#endif

#ifndef ENOTTY
#define ENOTTY			25	/* Inappropriate ioctl for device */
#endif

#ifndef ENXIO
#define ENXIO			 6	/* Device not configured */
#endif

#ifndef EPERM
#define EPERM			 1	/* Operation not permitted */
#endif

#ifndef EPIPE
#define EPIPE			32	/* Broken pipe */
#endif

#ifndef EROFS
#define EROFS			30	/* Read-only file system */
#endif

#ifndef ESIGPARM
#define ESIGPARM		26	/* Signal error */
#endif

#ifndef ESPIPE
#define ESPIPE			29	/* Illegal seek */
#endif

#ifndef ESRCH
#define ESRCH			 3	/* No such process */
#endif

#ifndef EUNKNOWN
#define EUNKNOWN		99	/* Unknown error */
#endif

#ifndef EXDEV
#define EXDEV			18	/* Cross-device link */
#endif


#if !defined(_EWL_ERRNO)

	_EWL_BEGIN_EXTERN_C

		#if _EWL_THREADSAFE && _EWL_LOCALDATA_AVAILABLE
			#include <ewl_thread_local_data.h>
			#define errno (_EWL_LOCALDATA(errno))

			#ifdef __CWCC__
				#if __CWCC__ >= 0x3200
					#ifndef __STARCORE__
					#pragma errno_name ...	/* not accessible via global */
					#endif
				#endif /* __CWCC__ >= 0x3200 */
			#endif /* __CWCC__ */
		#else
			_EWL_IMP_EXP_C extern int errno;
			#ifdef __cplusplus
				#define errno errno
			#endif
			#ifdef __CWCC__
				#if __CWCC__ >= 0x3200
					#ifndef __STARCORE__
					#pragma errno_name errno
					#endif
				#endif /* __CWCC__ >= 0x3200 */
			#endif /* __CWCC__ */
		#endif /* _EWL_THREADSAFE */

	_EWL_END_EXTERN_C

#endif /* _EWL_ERRNO */

#if defined(_EWL_EXPORT_CNAMES) && defined(__cplusplus) && defined(_EWL_USING_NAMESPACE)
#include <errno.h>
#endif /*  defined(_EWL_EXPORT_CNAMES) && defined(__cplusplus) && defined(_EWL_USING_NAMESPACE) */

#endif /* _EWL_USING_CW_C_HEADERS_ */

#endif /* _EWL_CERRNO */
